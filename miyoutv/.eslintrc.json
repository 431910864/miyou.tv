{
  "root": true,
  "globals": {
    "agGrid": true,
    "wcjsRenderer": true
  },
  "env": {
    "browser": true,
    "node": true
  },
  "plugins": [
    "node"
  ],
  "extends": [
    "airbnb-base/legacy",
    "plugin:node/recommended"
  ],
  "rules": {
    "func-names": "off",
    "node/no-unpublished-require": ["error", {
      "allowModules": ["electron"]
    }]
  }
}
